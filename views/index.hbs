<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>


<div>
  <h1>Posts</h1>

  <ul>
    {{#each posts}}
    <li>
      <a href="/post/{{this._id}}">
        <img src="{{this.picPath}}" alt="{{this.picName}}">
      </a>
      <h3>{{this.picName}}</h3>
      <p>{{this.content}}</p>
    </li>
    {{/each}}
  </ul>
</div>