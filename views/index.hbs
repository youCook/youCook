<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>


<div>
  <ul>
    {{#each posts}}
    <li>
      <a href="/post/post-info/{{this._id}}">
        <img src="{{this.picPath}}" alt="{{this.picName}}">
      </a>
      <h3>Author: {{this.picName}}</h3>
      <p>Content: {{this.content}}</p>
    </li>
    {{/each}}
  </ul>
</div>